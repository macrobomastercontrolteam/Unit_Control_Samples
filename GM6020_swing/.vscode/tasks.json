{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Import settings from Keil's v4 or v5 project",
      "type": "shell",
      "command": "python",
      "args": [
        "Scripts/_parse_Keil_project.py",
        "${relativeFile}",
        ".vscode/c_cpp_properties.json",
        "Scripts/_set_Keil_project_paths.bat"
      ],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": true
      },
      "problemMatcher": [
        "$tsc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Build by Keil",
      "type": "shell",
      "command": "${workspaceFolder}/Scripts/start_build.bat",
      "args": [
        "${command:cpptools.activeConfigName}"
      ],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": true
      },
      "problemMatcher": {
        "owner": "cpp",
        "fileLocation": [
          "relative",
          "${workspaceFolder}/MDK-ARM"
        ],
        "pattern": {
          "regexp": "^(.*)\\((\\d*)\\):\\s*(.rror|.arning|.nfo):\\s*(.*)$",
          "file": 1,
          "line": 2,
          "severity": 3,
          "message": 4,
          "column": -1
        }
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
